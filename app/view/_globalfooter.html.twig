<section id="modallauncher" role="dialog" tabindex="-1" class="slds-modal slds-fade-in-close slds-modal_large slds-app-launcher" aria-labelledby="header43" aria-modal="true" aria-describedby="modal-content-id-1">
    <div class="slds-modal__container">
        <header class="slds-modal__header slds-grid slds-grid_align-spread slds-grid_vertical-align-center">
            <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close" id="buttonlauncherClose">
                <svg class="slds-button__icon slds-button__icon_large" aria-hidden="true">
                    <use xlink:href="/app/public/assets/icons/utility-sprite/svg/symbols.svg#close"></use>
                </svg>
                <span class="slds-assistive-text">Close</span>
            </button>
            <h2 id="header43" class="slds-text-heading_medium">App launcher</h2>
            {% if modules.DASHBOARD %}<a class="slds-button slds-button_brand" href="/app/dashboard">Dashboard</a>  {% endif %}
        </header>
        <div class="slds-modal__content slds-app-launcher__content slds-p-around_medium" id="modal-content-id-1">
            <div class="slds-section slds-is-open">
                <h3 class="slds-section__title">
                    <button aria-controls="itemsContent" aria-expanded="true" class="slds-button slds-section__title-action">
                        <svg class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" aria-hidden="true">
                            <use xlink:href="/app/public/assets/icons/utility-sprite/svg/symbols.svg#switch"></use>
                        </svg>
                        <span class="slds-truncate" title="All Items">Modules E-GESCOM</span>
                    </button>
                </h3>
                <div aria-hidden="false" class="slds-section__content" id="itemsContent">
                    <ul class="slds-grid slds-wrap slds-gutters">

                        {% if modules.INFORMATIQUE  %}
                            <li class="slds-col_padded slds-size_1-of-1 slds-large-size_1-of-5 slds-m-bottom_medium">
                                <a href="/app/informatique" class="slds-box slds-box_link slds-box_x-small slds-media">
                                    <div class="slds-media__figure slds-media__figure_fixed-width slds-align_absolute-center slds-m-left_xx-small">

                                        <span class="slds-icon_container slds-icon-standard-service-appointment-capacity-usage">
                                                <svg class="slds-icon slds-icon_large" aria-hidden="true">
                                                    <use xlink:href="/app/public/assets/icons/standard-sprite/svg/symbols.svg#code_playground"></use>
                                                </svg>
                                            </span>

                                    </div>
                                    <div class="slds-media__body slds-border_left slds-p-around_small">
                                        <h2 class="slds-truncate slds-text-heading_small" title="Share the knowledge">Module Informatique</h2>
                                        <p class="slds-m-top_small">Module regroupant la partie informatique.</p>
                                    </div>
                                </a>
                            </li>
                        {% endif %}

                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="slds-backdrop slds-backdrop_close" id="modallauncherOverlay"></div>


<footer class="app-footer"></footer>
